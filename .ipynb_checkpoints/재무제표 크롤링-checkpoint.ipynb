       "\n",
       "var doPolling = function() {\n",
       "\tvar pollingApiUrl = \"https://polling.finance.naver.com/api/realtime?query=SERVICE_ITEM:\" + \"005935\"\n",
       "\n",
       "\tif (requestType == 'recent' && itemcodes != \"\") {\n",
